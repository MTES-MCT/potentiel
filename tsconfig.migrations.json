{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "dist/migrations",
    "sourceMap": false,
    "inlineSourceMap": true
  },
  "include": ["src/infra/sequelize/migrations/*.js", "src/infra/sequelize/migrations/*.ts"],
  "exclude": [
    "node_modules",
    "features",
    "**/*.spec.ts",
    "**/*.integration.ts",
    "**/*.stories.tsx",
    "**/__tests__/**/*",
    "scripts"
  ]
}