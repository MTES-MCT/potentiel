    "@types/passport": "^1.0.12",
    
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/uuid": "^9.0.3",
    
    "concurrently": "^7.6.0",
    "dotenv-cli": "^6.0.0",
    "glob": "^8.1.0",
    "lint-staged": "^13.3.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-prefix-selector": "^1.16.0",
    "prompts": "^2.4.2",
    "ts-loader": "^9.4.4",
