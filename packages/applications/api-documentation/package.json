{
  "name": "@potentiel-applications/api-documentation",
  "private": true,
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./schema": {
      "import": "./dist/api-schema.json"
    }
  },
  "scripts": {
    "codegen": "tsp compile ./src && ./scripts/patch-openapi.ts",
    "build": "npm run codegen && tsc"
  },
  "devDependencies": {
    "@typespec/compiler": "1.6.0",
    "@typespec/http": "1.6.0",
    "@typespec/http-server-js": "0.58.0-alpha.21",
    "@typespec/openapi3": "1.6.0",
    "@typespec/rest": "0.76.0"
  }
}
