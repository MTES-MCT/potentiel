{
  "name": "@potentiel/cli",
  "private": true,
  "type": "module",
  "bin": {
    "potentiel-cli": "./bin/run.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist tsconfig.tsbuildinfo"
  },
  "oclif": {
    "bin": "potentiel-cli",
    "commands": "./dist/commands",
    "dirname": "cli",
    "topicSeparator": " ",
    "hooks": {
      "init": "./dist/hooks/init",
      "prerun": "./dist/hooks/healthcheck/prerun",
      "finally": [
        "./dist/hooks/finally",
        "./dist/hooks/healthcheck/finally"
      ]
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.842.0",
    "@aws-sdk/lib-storage": "3.787.0",
    "@potentiel-applications/bootstrap": "workspace:*",
    "@potentiel-applications/document-builder": "workspace:*",
    "@potentiel-applications/routes": "workspace:*",
    "@potentiel-domain/common": "workspace:*",
    "@potentiel-domain/inmemory-referential": "workspace:*",
    "@potentiel-domain/periode": "workspace:*",
    "@potentiel-domain/projet": "workspace:*",
    "@potentiel-domain/reseau": "workspace:*",
    "@potentiel-domain/utilisateur": "workspace:*",
    "@potentiel-infrastructure/domain-adapters": "workspace:*",
    "@potentiel-infrastructure/ds-api-client": "workspace:*",
    "@potentiel-infrastructure/ore-client": "workspace:*",
    "@potentiel-libraries/csv": "workspace:*",
    "@potentiel-libraries/file-storage": "workspace:*",
    "@potentiel-libraries/monitoring": "workspace:*",
    "@potentiel-libraries/monads": "workspace:*",
    "@potentiel-libraries/pg-helpers": "workspace:*",
    "@potentiel-infrastructure/pg-event-sourcing": "workspace:*",
    "@potentiel-infrastructure/pg-projection-read": "workspace:*",
    "@potentiel-infrastructure/email": "workspace:*",
    "@potentiel-statistiques/statistiques-publiques": "workspace:*",
    "@oclif/core": "4.5.0",
    "cockatiel": "3.2.1",
    "dotenv": "16.5.0",
    "mediateur": "catalog:",
    "winston": "3.17.0",
    "winston-elasticsearch": "0.19.0",
    "winston-transport": "4.9.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@potentiel-config/tsconfig": "workspace:*",
    "diff": "8.0.2",
    "oclif": "4.20.4",
    "tsx": "4.20.4"
  }
}
