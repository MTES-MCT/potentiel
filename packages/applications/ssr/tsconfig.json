{
  "compilerOptions": {
    "target": "es2022",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"],
  "references": [
    {
      "path": "../../domain/appel-offre"
    },
    {
      "path": "../../domain/entity"
    },
    {
      "path": "../../domain/lauréat"
    },
    {
      "path": "../../domain/réseau"
    },
    {
      "path": "../../domain/tâche"
    },
    {
      "path": "../../domain/projet"
    },
    {
      "path": "../../domain/document"
    },
    {
      "path": "../../domain/utilisateur"
    },
    {
      "path": "../../libraries/csv"
    },
    {
      "path": "../../libraries/monads"
    },
    {
      "path": "../../libraries/monitoring"
    },
    {
      "path": "../../infrastructure/filigrane-facile-client"
    },
    {
      "path": "../../infrastructure/geo-api-client"
    },
    {
      "path": "../api-documentation"
    },
    {
      "path": "../bootstrap"
    },
    {
      "path": "../document-builder"
    },
    {
      "path": "../request-context"
    }
  ]
}
