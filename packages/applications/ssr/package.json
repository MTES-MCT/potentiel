{
  "name": "@potentiel-applications/ssr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run prebuild && tsx -r dotenv/config src/server.ts",
    "start": "NODE_ENV=production node -r dotenv/config dist/server.js",
    "build": "pnpm run prebuild && pnpm run build:server && next build",
    "build:server": "tsc -p tsconfig.server.json",
    "lint": "next lint",
    "lint:fix": "eslint . --fix",
    "prebuild": "react-dsfr update-icons --silent",
    "test": "glob -c \"tsx --test\" \"src/**/*.test.ts\"",
    "clean": "rm -rf dist tsconfig.tsbuildinfo .next"
  },
