    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "neverthrow": "^6.2.2",
    "next": "^14.2.15",
    "node-mailjet": "^6.0.6",
    "pg-connection-string": "^2.7.0",
    "pizzip": "^3.1.7",
    "querystring": "^0.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "runtypes": "^6.7.0",
    "sanitize-filename": "^1.6.3",
    "sequelize": "^6.37.4",
    "sequelize-cli": "^6.6.2",
    "util": "^0.12.5",
    "utility-types": "^3.11.0",
