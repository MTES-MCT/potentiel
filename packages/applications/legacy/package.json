    "esbuild-loader": "^2.21.0",
    "glob": "^11.0.0",
    "jest": "^29.6.4",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "path-browserify": "^1.0.1",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-prefix-selector": "^1.16.1",
