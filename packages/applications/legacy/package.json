{
  "name": "@potentiel-applications/legacy",
  "description": "Suivi des Projets d'Energies Renouvelables",
  "sideEffects": false,
  "main": "index.js",
  "scripts": {
    "type:check": "tsc --noEmit",
    "start": "node ./dist/src/index.js",
    "build": "npm run build:front && npm run build:css && npm run build:back",
    "build:back": "npx --node-options=\"--max-old-space-size=8192\" tsc --build tsconfig.build.json && cp -R src/public dist/src && cp -R src/views/maintenance.html dist/src/views",
    "build:css": "tailwindcss -i ./src/views/index.css -o ./src/public/css/index.css",
    "build:front": "webpack --config=webpack.config.js",
    "lint": "eslint ./src --ext .ts,.tsx",
    "format": "prettier './src/**/*.{ts,tsx,css}' --write",
    "clean": "rm -rf dist tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@hqoss/monads": "0.5.0",
    "@potentiel-applications/bootstrap": "*",
    "@potentiel-applications/api": "*",
    "@potentiel-applications/request-context": "*",
    "@potentiel-applications/routes": "*",
    "@potentiel-applications/subscribers": "*",
    "@potentiel-domain/appel-offre": "*",
    "@potentiel-domain/common": "*",
    "@potentiel-domain/inmemory-referential": "*",
    "@potentiel-domain/projet": "*",
    "@potentiel-domain/reseau": "*",
    "@potentiel-infrastructure/pg-event-sourcing": "*",
    "@potentiel-libraries/monads": "*",
    "@potentiel-libraries/monitoring": "*",
    "@potentiel-domain/utilisateur": "*",
    "@potentiel-libraries/csv": "*",
    "@sentry/node": "8.55.0",
    "date-fns": "2.30.0",
    "dotenv": "16.5.0",
    "express": "4.22.1",
    "express-async-handler": "1.2.0",
    "helmet": "6.2.0",
    "iconv-lite": "0.6.3",
    "ioredis": "4.29.1",
    "isemail": "3.2.0",
    "mediateur": "0.3.0",
    "mime-types": "2.1.35",
    "moment": "2.30.1",
    "moment-timezone": "0.5.48",
    "morgan": "1.10.1",
    "neverthrow": "6.2.2",
    "next": "14.2.35",
    "pg-connection-string": "2.9.1",
    "querystring": "0.2.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.5.0",
    "runtypes": "6.7.0",
    "sanitize-filename": "1.6.3",
    "sequelize": "6.37.7",
    "utility-types": "3.11.0",
    "uuid": "9.0.1",
    "vm-browserify": "1.1.2",
    "winston-elasticsearch": "0.19.0",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "7.0.2",
    "@types/express": "4.17.21",
    "@types/ioredis": "4.28.10",
    "@types/mime-types": "2.1.4",
    "@types/morgan": "1.9.9",
    "@types/node": "20.17.30",
    "@types/react": "18.3.20",
    "@types/react-dom": "18.3.6",
    "@types/uuid": "9.0.8",
    "concurrently": "7.6.0",
    "dotenv-webpack": "8.1.0",
    "esbuild-loader": "4.3.0",
    "glob": "11.1.0",
    "path-browserify": "1.0.1",
    "postcss-import": "15.1.0",
    "tailwindcss": "3.4.17",
    "webpack": "5.99.5",
    "webpack-cli": "4.10.0"
  }
