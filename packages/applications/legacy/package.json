{
  "name": "@potentiel-applications/legacy",
  "version": "0.0.0",
  "description": "Suivi des Projets d'Energies Renouvelables",
  "sideEffects": false,
  "main": "index.js",
  "scripts": {
    "type:check": "tsc --noEmit",
    "start": "node ./dist/src/index.js",
    "start:dev": "npm run migrate && npm run seed && concurrently \"ts-node-dev --transpile-only src/index.ts\" \"webpack --config=webpack.config.js --watch\" \"npm run build:css -- --watch\"",
    "build": "npm run build:front && npm run build:css && npm run build:back && npm run build:migration",
    "build:back": "npx --node-options=\"--max-old-space-size=8192\" tsc --build tsconfig.build.json && cp -R src/public dist/src && cp -R src/views/template dist/src/views && cp -R src/views/maintenance.html dist/src/views",
    "build:css": "tailwindcss -i ./src/views/index.css -o ./src/public/css/index.css",
    "build:front": "webpack --config=webpack.config.js",
    "build:migration": "rimraf dist/migrations && tsc --build tsconfig.migrations.json",
    "premigrate": "npm run build:migration",
    "migrate": "sequelize-cli db:migrate",
    "premigrate:undo": "npm run build:migration",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "preseed": "npm run build:migration",
    "seed": "sequelize-cli db:seed:all",
    "lint": "eslint ./src --ext .ts,.tsx",
    "format": "prettier './src/**/*.{ts,tsx,css}' --write",
    "test": "export APPLICATION_STAGE=test && npm run migrate && jest --config=jest.config.js --detectOpenHandles"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@hqoss/monads": "^0.5.0",
    "@json2csv/plainjs": "^6.1.3",
    "@keycloak/keycloak-admin-client": "^23.0.4",
    "@potentiel-applications/bootstrap": "*",
    "@potentiel-applications/feature-flags": "*",
    "@potentiel-applications/routes": "*",
    "@potentiel-domain/appel-offre": "*",
    "@potentiel-domain/common": "*",
    "@potentiel-domain/inmemory-referential": "*",
    "@potentiel-domain/laureat": "*",
    "@potentiel-domain/reseau": "*",
    "@potentiel-infrastructure/pg-event-sourcing": "*",
    "@react-pdf/renderer": "^3.1.13",
    "@sentry/node": "^7.112.2",
    "aws-sdk": "^2.1450.0",
    "connect-session-sequelize": "^7.1.7",
    "cookie-parser": "^1.4.6",
    "csv-parse": "^5.5.0",
    "date-fns": "^2.30.0",
    "docxtemplater": "^3.38.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.20.1",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.17.3",
    "helmet": "^6.2.0",
    "iconv-lite": "^0.6.3",
    "ioredis": "^4.28.5",
    "isemail": "^3.2.0",
    "json2csv": "^5.0.7",
    "jsonwebtoken": "^9.0.2",
    "keycloak-connect": "^23.0.4",
    "mediateur": "^0.1.3",
    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "neverthrow": "^6.0.0",
    "next": "^14.1.1",
    "node-mailjet": "^6.0.5",
    "pg-connection-string": "^2.6.2",
    "pizzip": "^3.1.4",
    "querystring": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "runtypes": "^6.7.0",
    "sanitize-filename": "^1.6.3",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "tailwindcss": "^3.3.3",
    "util": "^0.12.5",
    "utility-types": "^3.10.0",
    "uuid": "^9.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@jest/globals": "^29.6.4",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/ioredis": "^4.28.10",
    "@types/mime-types": "^2.1.1",
    "@types/morgan": "^1.9.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.17.14",
    "@types/node-mailjet": "^3.3.9",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/uuid": "^9.0.3",
    "concurrently": "^7.6.0",
    "dotenv-webpack": "^8.1.0",
    "esbuild-loader": "^2.21.0",
    "glob": "^8.1.0",
    "jest": "^29.6.4",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "path-browserify": "^1.0.1",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-prefix-selector": "^1.16.0",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "wait-for-expect": "^3.0.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^4.10.0"
  }
}
