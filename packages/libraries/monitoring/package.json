{
  "name": "@potentiel-libraries/monitoring",
  "description": "Potentiel Monitoring",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "tsx --test ./**/*.test.ts",
    "clean": "rm -rf dist tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@sentry/core": "8.55.0",
    "@sentry/nextjs": "8.55.0",
    "winston": "3.17.0"
  },
  "devDependencies": {
    "@potentiel-config/tsconfig": "*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./sentry": {
      "types": "./dist/sentry/index.d.ts",
      "default": "./dist/sentry/index.js"
    },
    "./winston": {
      "types": "./dist/winston/index.d.ts",
      "default": "./dist/winston/index.js"
    }
  }
}
