    "pizzip": "^3.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.10.1",
    "runtypes": "^6.7.0",
    "sanitize-filename": "^1.6.3",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "tailwindcss": "^3.3.3",
    "util": "^0.12.5",
    "utility-types": "^3.10.0",
    "uuid": "^9.0.0",
