    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "tailwindcss": "^3.0.13",
    "ts-loader": "^8.1.0",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.12.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "ttypescript": "^1.5.13",
    "typescript-transform-paths": "^3.3.1",
    "typescript": "^4.5.4",
    "utility-types": "^3.10.0",
