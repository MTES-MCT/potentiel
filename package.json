    "docxtemplater": "^3.21.1",
    "dotenv": "^11.0.0",
    "esbuild-loader": "^2.18.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-session": "^1.17.1",
    "helmet": "^4.4.1",
    "iconv-lite": "^0.6.2",
    "ioredis": "^4.28.1",
    "isemail": "^3.2.0",
