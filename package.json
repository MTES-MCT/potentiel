    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.17.3",
    "helmet": "^6.2.0",
    "iconv-lite": "^0.6.3",
    "ioredis": "^4.28.5",
