{
  "name": "potentiel.beta.gouv.fr",
  "version": "2.148.3",
  "sideEffects": false,
  "description": "Suivi des Projets d'Energies Renouvelables",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:front && npm run build:css && npm run build:dsfr && npm run build:back",
    "build:back": "ttsc --build tsconfig.build.json && cp -R src/public dist/src && cp -R src/views/template dist/src/views",
    "build:front": "webpack --config=webpack.config.js",
    "build:css": "npx tailwindcss -i ./src/views/index.css -o ./src/public/css/index.css",
    "build:dsfr": "postcss ./src/views/dsfr.css -o ./src/public/css/dsfr.css",
    "generate:doc": "ts-node ./devtools/generateEventsDoc",
    "install": "ts-node ./scripts/postInstall.ts",
    "test": "NODE_ENV=unit-test jest --config=jest.unit.config.js --detectOpenHandles",
    "test:watch": "NODE_ENV=test TEST=unit jest --config=jest.unit.config.js --detectOpenHandles --watch",
    "test-int": "NODE_ENV=test jest --config=jest.integration.config.js --detectOpenHandles --runInBand",
    "test-int:watch": "NODE_ENV=test jest --config=jest.integration.config.js --detectOpenHandles --watch",
    "launch-test-db": "export NODE_ENV=test && export COMPOSE_PROJECT_NAME=potentiel_tests && docker-compose down --remove-orphans && docker-compose -f ./docker-compose-integration.yml up -d && until docker exec potentiel_db_tests_integration pg_isready -U testuser -d potentiel_test; do sleep 1; done",
    "migrate": "rm -rf dist/migrations && ttsc --build tsconfig.migrations.json && npx sequelize-cli db:migrate --migrations-path=dist/migrations/src/infra/sequelize/migrations",
    "seed": "npx sequelize-cli db:seed:all",
    "test-db": "export NODE_ENV=test && npm run launch-test-db && sleep 2 && npm run migrate",
    "dev-db": "docker compose down --remove-orphans && docker compose up -d",
    "storybook": "concurrently \"npm run watch:css\" \"BASE_URL=http://localhost:6006 start-storybook -p 6006\"",
    "build-storybook": "build-storybook",
    "storybook-build": "build-storybook-c .storybook -o docs",
    "start": "node ./dist/src/index.js",
    "watch": "concurrently \"npm run watch:back\" \"npm run watch:front\" \"npm run watch:css\" \"npm run build:dsfr\"",
    "watch:back": "ts-node-dev --transpile-only -r tsconfig-paths/register src/index.ts",
    "watch:front": "webpack --config=webpack.config.js --watch",
    "watch:css": "npm run build:css -- --watch",
    "generate": "./devtools/bin/index.js",
    "add:event": "npm run generate -- event",
    "add:e": "npm run add:event",
    "add:usecase": "npm run generate -- usecase",
    "add:u": "npm run add:usecase",
    "add:projection": "npm run generate -- projection",
    "add:p": "npm run add:projection",
    "add:query": "npm run generate -- query",
    "add:q": "npm run add:query"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MTES-MCT/potentiel.git"
  },
  "author": "@paduc",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/MTES-MCT/potentiel/issues"
  },
  "homepage": "https://github.com/MTES-MCT/potentiel#readme",
